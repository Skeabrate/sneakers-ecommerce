{
    "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        /* "react-hooks/exhaustive-deps": 0 */
        "prettier/prettier": ["error", {
            "singleQuote": true,
            "useTabs": true
        }]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint"
}